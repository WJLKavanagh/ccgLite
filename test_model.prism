mdp

//Player 1:
const int p1_hea = 8;
const int p1_dmg = 1;

	//Card 1: KNIGHT
const int p1c1_hea = 3;
const int p1c1_dmg = 2;
const double p1c1_acc = 0.75
;
	//Card 2: ROGUE
const int p1c2_hea = 3;
const int p1c2_dmg = 1;
const double p1c2_acc = 0.9
;
	//Card 3: PRIEST
const int p1c3_hea = 2;
const int p1c3_dmg = 2;
const double p1c3_acc = 0.7
;
//Player 2:
const int p2_hea = 8;
const int p2_dmg = 1;

	//Card 1: KNIGHT
const int p2c1_hea = 3;
const int p2c1_dmg = 2;
const double p2c1_acc = 0.75
;
	//Card 2: GUARDIAN
const int p2c2_hea = 4;
const int p2c2_dmg = 1;
const double p2c2_acc = 0.8
;
	//Card 3: ARCHER
const int p2c3_hea = 2;
const int p2c3_dmg = 1;
const double p2c3_acc = 0.8
;

module game

	p1 : [0..p1_hea] init p1_hea;	//player 1 hero health
	p2 : [0..p2_hea] init p2_hea;	//player 2 hero health
	p1c1 : [-1..4] init -1; 	//card status/health, -1 = card in hand, 0 = card dead.
	p1c2 : [-1..4] init -1;
	p1c3 : [-1..4] init -1;
	p2c1 : [-1..4] init -1;
	p2c2 : [-1..4] init -1;
	p2c3 : [-1..4] init -1;
	turn_clock : [0..2] init 0;	//Whose turn is it, 0 = flip coin
	action : [0..39] init 0;	//What action has been chosen, 0 = action decision

	[flip_coin] turn_clock = 0 ->
		0.5 : (turn_clock' = 1) + 0.5 : (turn_clock' = 2);
// Player 1: Free strategy...
	[player_1_turn] action = 0 & turn_clock = 1 & p1c1 = -1 ->
		(action' = 1);
	[player_1_turn] action = 0 & turn_clock = 1 & p1c2 = -1 ->
		(action' = 2);
	[player_1_turn] action = 0 & turn_clock = 1 & p1c3 = -1 ->
		(action' = 3);
	[player_1_turn] action = 0 & turn_clock = 1 & p2c1 < 1 & p2c2 < 1 & p2c3 < 1 ->
		(action' = 7);
	[player_1_turn] action = 0 & turn_clock = 1 & p2c1 > 0 ->
		(action' = 8);
	[player_1_turn] action = 0 & turn_clock = 1 & p2c2 > 0 ->
		(action' = 9);
	[player_1_turn] action = 0 & turn_clock = 1 & p2c3 > 0 ->
		(action' = 10);
	[player_1_turn] action = 0 & turn_clock = 1 & p1c1 > 0 & p2 > 0 ->
		(action' = 15);
	[player_1_turn] action = 0 & turn_clock = 1 & p1c1 > 0 & p2c1 > 0 ->
		(action' = 16);
	[player_1_turn] action = 0 & turn_clock = 1 & p1c1 > 0 & p2c2 > 0 ->
		(action' = 17);
	[player_1_turn] action = 0 & turn_clock = 1 & p1c1 > 0 & p2c3 > 0 ->
		(action' = 18);
	[player_1_turn] action = 0 & turn_clock = 1 & p1c2 > 0 & p2 > 0 ->
		(action' = 19);
	[player_1_turn] action = 0 & turn_clock = 1 & p1c2 > 0 & p2c1 > 0 ->
		(action' = 20);
	[player_1_turn] action = 0 & turn_clock = 1 & p1c2 > 0 & p2c2 > 0 ->
		(action' = 21);
	[player_1_turn] action = 0 & turn_clock = 1 & p1c2 > 0 & p2c3 > 0 ->
		(action' = 22);
	[player_1_turn] action = 0 & turn_clock = 1 & p1c3 > 0 & p1 < p1_hea ->
		(action' = 23);
	[player_1_turn] action = 0 & turn_clock = 1 & p1c3 > 0 & p1c1 > 0 & p1c1 < p1c1_hea ->
		(action' = 24);
	[player_1_turn] action = 0 & turn_clock = 1 & p1c3 > 0 & p1c2 > 0 & p1c2 < p1c2_hea ->
		(action' = 25);
	[player_1_turn] action = 0 & turn_clock = 1 & p1c3 > 0 & p1c3 < p1c3_hea ->
		(action' = 26);

// Player 2: Free strategy...
	[player_2_turn] action = 0 & turn_clock = 2 & p2c1 = -1 ->
		(action' = 4);
	[player_2_turn] action = 0 & turn_clock = 2 & p2c2 = -1 ->
		(action' = 5);
	[player_2_turn] action = 0 & turn_clock = 2 & p2c3 = -1 ->
		(action' = 6);
	[player_2_turn] action = 0 & turn_clock = 2 & p1c1 < 1 & p1c2 < 1 & p1c3 < 1 ->
		(action' = 11);
	[player_2_turn] action = 0 & turn_clock = 2 & p1c1 > 0 ->
		(action' = 12);
	[player_2_turn] action = 0 & turn_clock = 2 & p1c2 > 0 ->
		(action' = 13);
	[player_2_turn] action = 0 & turn_clock = 2 & p1c3 > 0 ->
		(action' = 14);
	[player_2_turn] action = 0 & turn_clock = 2 & p2c1 > 0 & p1 > 0 ->
		(action' = 27);
	[player_2_turn] action = 0 & turn_clock = 2 & p2c1 > 0 & p1c1 > 0 ->
		(action' = 28);
	[player_2_turn] action = 0 & turn_clock = 2 & p2c1 > 0 & p1c2 > 0 ->
		(action' = 29);
	[player_2_turn] action = 0 & turn_clock = 2 & p2c1 > 0 & p1c3 > 0 ->
		(action' = 30);
	[player_2_turn] action = 0 & turn_clock = 2 & p2c2 > 0 & p1 > 0 ->
		(action' = 31);
	[player_2_turn] action = 0 & turn_clock = 2 & p2c2 > 0 & p1c1 > 0 ->
		(action' = 32);
	[player_2_turn] action = 0 & turn_clock = 2 & p2c2 > 0 & p1c2 > 0 ->
		(action' = 33);
	[player_2_turn] action = 0 & turn_clock = 2 & p2c2 > 0 & p1c3 > 0 ->
		(action' = 34);
	[player_2_turn] action = 0 & turn_clock = 2 & p2c3 > 0 ->
		(action' = 35);

// Actions
	[p1_draw_p1c1]		action = 1 & p1c1 = -1 ->
					(p1c1' = p1c1_hea) & (action' = 39);

	[p1_draw_p1c2]		action = 2 & p1c2 = -1 ->
					(p1c2' = p1c2_hea) & (action' = 39);

	[p1_draw_p1c3]		action = 3 & p1c3 = -1 ->
					(p1c3' = p1c3_hea) & (action' = 39);

	[p2_draw_p2c1]		action = 4 & p2c1 = -1 ->
					(p2c1' = p2c1_hea) & (action' = 39);

	[p2_draw_p2c2]		action = 5 & p2c2 = -1 ->
					(p2c2' = p2c2_hea) & (action' = 39);

	[p2_draw_p2c3]		action = 6 & p2c3 = -1 ->
					(p2c3' = p2c3_hea) & (action' = 39);

	[p1_hero_attack_p2]	action = 7& p2 > 0 ->
					(p2' = p2 - 1) & (action' = 39);
	[p1_hero_attack_p2c1]	action = 8& p2c1 > 0 ->
					(p2c1' = p2c1 - 1) & (action' = 39);
	[p1_hero_attack_p2c2]	action = 9& p2c2 > 0 ->
					(p2c2' = p2c2 - 1) & (action' = 39);
	[p1_hero_attack_p2c3]	action = 10& p2c3 > 0 ->
					(p2c3' = p2c3 - 1) & (action' = 39);
	[p2_hero_attack_p1]	action = 11& p1 > 0 ->
					(p1' = p1 - 1) & (action' = 39);
	[p2_hero_attack_p1c1]	action = 12& p1c1 > 0 ->
					(p1c1' = p1c1 - 1) & (action' = 39);
	[p2_hero_attack_p1c2]	action = 13& p1c2 > 0 ->
					(p1c2' = p1c2 - 1) & (action' = 39);
	[p2_hero_attack_p1c3]	action = 14& p1c3 > 0 ->
					(p1c3' = p1c3 - 1) & (action' = 39);
	[p1c1_attack_p2]	action = 15 & p2 > p1c1_dmg ->
		p1c1_acc 		: (p2' = p2 - p1c1_dmg) & (action' = 39) +
		1 - p1c1_acc 		: (action' = 39);
	[p1c1_attack_p2]	action = 15 & p2 <= p1c1_dmg ->
		p1c1_acc 		: (p2' = 0) & (action' = 39) +
		1 - p1c1_acc 		: (action' = 39);

	[p1c1_attack_p2c1]	action = 16 & p2c1 > p1c1_dmg ->
		p1c1_acc 		: (p2c1' = p2c1 - p1c1_dmg) & (action' = 39) +
		1 - p1c1_acc 		: (action' = 39);
	[p1c1_attack_p2c1]	action = 16 & p2c1 <= p1c1_dmg ->
		p1c1_acc 		: (p2c1' = 0) & (action' = 39) +
		1 - p1c1_acc 		: (action' = 39);

	[p1c1_attack_p2c2]	action = 17 & p2c2 > p1c1_dmg ->
		p1c1_acc 		: (p2c2' = p2c2 - p1c1_dmg) & (action' = 39) +
		1 - p1c1_acc 		: (action' = 39);
	[p1c1_attack_p2c2]	action = 17 & p2c2 <= p1c1_dmg ->
		p1c1_acc 		: (p2c2' = 0) & (action' = 39) +
		1 - p1c1_acc 		: (action' = 39);

	[p1c1_attack_p2c3]	action = 18 & p2c3 > p1c1_dmg ->
		p1c1_acc 		: (p2c3' = p2c3 - p1c1_dmg) & (action' = 39) +
		1 - p1c1_acc 		: (action' = 39);
	[p1c1_attack_p2c3]	action = 18 & p2c3 <= p1c1_dmg ->
		p1c1_acc 		: (p2c3' = 0) & (action' = 39) +
		1 - p1c1_acc 		: (action' = 39);

	[p1c2_attack_p2]	action = 19 & p2 > p1c2_dmg ->
		p1c2_acc 		: (p2' = p2 - p1c2_dmg) & (action' = 39) +
		1 - p1c2_acc 		: (action' = 39);
	[p1c2_attack_p2]	action = 19 & p2 <= p1c2_dmg ->
		p1c2_acc 		: (p2' = 0) & (action' = 39) +
		1 - p1c2_acc 		: (action' = 39);

	[p1c2_attack_p2c1]	action = 20 & p2c1 > p1c2_dmg ->
		p1c2_acc 		: (p2c1' = p2c1 - p1c2_dmg) & (action' = 39) +
		1 - p1c2_acc 		: (action' = 39);
	[p1c2_attack_p2c1]	action = 20 & p2c1 <= p1c2_dmg ->
		p1c2_acc 		: (p2c1' = 0) & (action' = 39) +
		1 - p1c2_acc 		: (action' = 39);

	[p1c2_attack_p2c2]	action = 21 & p2c2 > p1c2_dmg ->
		p1c2_acc 		: (p2c2' = p2c2 - p1c2_dmg) & (action' = 39) +
		1 - p1c2_acc 		: (action' = 39);
	[p1c2_attack_p2c2]	action = 21 & p2c2 <= p1c2_dmg ->
		p1c2_acc 		: (p2c2' = 0) & (action' = 39) +
		1 - p1c2_acc 		: (action' = 39);

	[p1c2_attack_p2c3]	action = 22 & p2c3 > p1c2_dmg ->
		p1c2_acc 		: (p2c3' = p2c3 - p1c2_dmg) & (action' = 39) +
		1 - p1c2_acc 		: (action' = 39);
	[p1c2_attack_p2c3]	action = 22 & p2c3 <= p1c2_dmg ->
		p1c2_acc 		: (p2c3' = 0) & (action' = 39) +
		1 - p1c2_acc 		: (action' = 39);

	[p1c3_heal_p1]		action = 23 & p1 >= (p1 - p1c3_dmg) ->
		p1c3_acc		: (p1' = p1_hea) & (action' = 39) +
		1 -p1c3_acc		: (action' = 39);
	[p1c3_heal_p1]		action = 23 & p1 < p1 - p1c3_dmg ->
		p1c3_acc		: (p1' = p1 + p1c3_dmg) & (action' = 39)+ 
		1 -p1c3_acc		: (action' = 39);

	[p1c3_heal_c1]		action = 24 & p1 >= (p1 - p1c3_dmg) ->
		p1c3_acc		: (p1' = p1_hea) & (action' = 39) +
		1 -p1c3_acc		: (action' = 39);
	[p1c3_heal_c1]		action = 24 & p1 < p1 - p1c3_dmg ->
		p1c3_acc		: (p1' = p1 + p1c3_dmg) & (action' = 39)+ 
		1 -p1c3_acc		: (action' = 39);

	[p1c3_heal_c2]		action = 25 & p1 >= (p1 - p1c3_dmg) ->
		p1c3_acc		: (p1' = p1_hea) & (action' = 39) +
		1 -p1c3_acc		: (action' = 39);
	[p1c3_heal_c2]		action = 25 & p1 < p1 - p1c3_dmg ->
		p1c3_acc		: (p1' = p1 + p1c3_dmg) & (action' = 39)+ 
		1 -p1c3_acc		: (action' = 39);

	[p1c3_heal_c3]		action = 26 & p1 >= (p1 - p1c3_dmg) ->
		p1c3_acc		: (p1' = p1_hea) & (action' = 39) +
		1 -p1c3_acc		: (action' = 39);
	[p1c3_heal_c3]		action = 26 & p1 < p1 - p1c3_dmg ->
		p1c3_acc		: (p1' = p1 + p1c3_dmg) & (action' = 39)+ 
		1 -p1c3_acc		: (action' = 39);

	[p2c1_attack_p1]	action = 27 & p1 > p2c1_dmg ->
		p2c1_acc 		: (p1' = p1 - p2c1_dmg) & (action' = 39) +
		1 - p2c1_acc 		: (action' = 39);
	[p2c1_attack_p1]	action = 27 & p1 <= p2c1_dmg ->
		p2c1_acc 		: (p1' = 0) & (action' = 39) +
		1 - p2c1_acc 		: (action' = 39);

	[p2c1_attack_p1c1]	action = 28 & p1c1 > p2c1_dmg ->
		p2c1_acc 		: (p1c1' = p1c1 - p2c1_dmg) & (action' = 39) +
		1 - p2c1_acc 		: (action' = 39);
	[p2c1_attack_p1c1]	action = 28 & p1c1 <= p2c1_dmg ->
		p2c1_acc 		: (p1c1' = 0) & (action' = 39) +
		1 - p2c1_acc 		: (action' = 39);

	[p2c1_attack_p1c2]	action = 29 & p1c2 > p2c1_dmg ->
		p2c1_acc 		: (p1c2' = p1c2 - p2c1_dmg) & (action' = 39) +
		1 - p2c1_acc 		: (action' = 39);
	[p2c1_attack_p1c2]	action = 29 & p1c2 <= p2c1_dmg ->
		p2c1_acc 		: (p1c2' = 0) & (action' = 39) +
		1 - p2c1_acc 		: (action' = 39);

	[p2c1_attack_p1c3]	action = 30 & p1c3 > p2c1_dmg ->
		p2c1_acc 		: (p1c3' = p1c3 - p2c1_dmg) & (action' = 39) +
		1 - p2c1_acc 		: (action' = 39);
	[p2c1_attack_p1c3]	action = 30 & p1c3 <= p2c1_dmg ->
		p2c1_acc 		: (p1c3' = 0) & (action' = 39) +
		1 - p2c1_acc 		: (action' = 39);

	[p2c2_attack_p1]	action = 31 & p1 > p2c2_dmg & p2 < p2_hea ->
		p2c2_acc		: (p1' = p1 - p2c2_dmg) & (p2' = p2 + 1) & (action' = 39) + 
		1 - p2c2_acc		: (action'= 39);
	[p2c2_attack_p1]	action = 31 & p1 > p2c2_dmg & p2 = p2_hea ->
		p2c2_acc		: (p1' = p1 - p2c2_dmg) & (action' = 39) +
		1 - p2c2_acc		: (action'= 39);
	[p2c2_attack_p1]	action = 31 & p1 <= p2c2_dmg & p2 < p2_hea ->
		p2c2_acc		: (p1' = 0) & (p2' = p2 + 1) & (action' = 39) + 
		1 - p2c2_acc		: (action'= 39);
	[p2c2_attack_p1]	action = 31 & p1 <= p2c2_dmg & p2 = p2_hea ->
		p2c2_acc		: (p1' = 0) & (action' = 39) +
		1 - p2c2_acc		: (action' = 39);

	[p2c2_attack_p1c1]	action = 32 & p1c1 > p2c2_dmg & p2 < p2_hea ->
		p2c2_acc		: (p1c1' = p1c1 - p2c2_dmg) & (p2' = p2 + 1) & (action' = 39) + 
		1 - p2c2_acc		: (action'= 39);
	[p2c2_attack_p1c1]	action = 32 & p1c1 > p2c2_dmg & p2 = p2_hea ->
		p2c2_acc		: (p1c1' = p1c1 - p2c2_dmg) & (action' = 39) +
		1 - p2c2_acc		: (action'= 39);
	[p2c2_attack_p1c1]	action = 32 & p1c1 <= p2c2_dmg & p2 < p2_hea ->
		p2c2_acc		: (p1c1' = 0) & (p2' = p2 + 1) & (action' = 39) + 
		1 - p2c2_acc		: (action'= 39);
	[p2c2_attack_p1c1]	action = 32 & p1c1 <= p2c2_dmg & p2 = p2_hea ->
		p2c2_acc		: (p1c1' = 0) & (action' = 39) +
		1 - p2c2_acc		: (action' = 39);

	[p2c2_attack_p1c2]	action = 33 & p1c2 > p2c2_dmg & p2 < p2_hea ->
		p2c2_acc		: (p1c2' = p1c2 - p2c2_dmg) & (p2' = p2 + 1) & (action' = 39) + 
		1 - p2c2_acc		: (action'= 39);
	[p2c2_attack_p1c2]	action = 33 & p1c2 > p2c2_dmg & p2 = p2_hea ->
		p2c2_acc		: (p1c2' = p1c2 - p2c2_dmg) & (action' = 39) +
		1 - p2c2_acc		: (action'= 39);
	[p2c2_attack_p1c2]	action = 33 & p1c2 <= p2c2_dmg & p2 < p2_hea ->
		p2c2_acc		: (p1c2' = 0) & (p2' = p2 + 1) & (action' = 39) + 
		1 - p2c2_acc		: (action'= 39);
	[p2c2_attack_p1c2]	action = 33 & p1c2 <= p2c2_dmg & p2 = p2_hea ->
		p2c2_acc		: (p1c2' = 0) & (action' = 39) +
		1 - p2c2_acc		: (action' = 39);

	[p2c2_attack_p1c3]	action = 34 & p1c3 > p2c2_dmg & p2 < p2_hea ->
		p2c2_acc		: (p1c3' = p1c3 - p2c2_dmg) & (p2' = p2 + 1) & (action' = 39) + 
		1 - p2c2_acc		: (action'= 39);
	[p2c2_attack_p1c3]	action = 34 & p1c3 > p2c2_dmg & p2 = p2_hea ->
		p2c2_acc		: (p1c3' = p1c3 - p2c2_dmg) & (action' = 39) +
		1 - p2c2_acc		: (action'= 39);
	[p2c2_attack_p1c3]	action = 34 & p1c3 <= p2c2_dmg & p2 < p2_hea ->
		p2c2_acc		: (p1c3' = 0) & (p2' = p2 + 1) & (action' = 39) + 
		1 - p2c2_acc		: (action'= 39);
	[p2c2_attack_p1c3]	action = 34 & p1c3 <= p2c2_dmg & p2 = p2_hea ->
		p2c2_acc		: (p1c3' = 0) & (action' = 39) +
		1 - p2c2_acc		: (action' = 39);

	[p2c3_attacks_all]	action = 35 & p1c1 > 0 & p1c2 > 0 & p1c3 > 0 & p1 > 0 ->
		pow(p2c3_acc,4)		: (p1' = p1 - p2c3_dmg) & (p1c1' = p1c1 - p2c3_dmg) & 
					(p1c2' = p1c2 - p2c3_dmg) & (p1c3' = p1c3 - p2c3_dmg) & (action' = 39) +
		pow(p2c3_acc,3)*pow(1-p2c3_acc,1)
					: (p1c1' = p1c1 - p2c3_dmg) & (p1c2' = p1c2 - p2c3_dmg) & 
					(p1c3' = p1c3 - p2c3_dmg) & (action' = 39) + 
		pow(p2c3_acc,3)*pow(1-p2c3_acc,1)
					: (p1' = p1 - p2c3_dmg) & (p1c2' = p1c2 - p2c3_dmg) & 
					(p1c3' = p1c3 - p2c3_dmg) & (action' = 39) + 
		pow(p2c3_acc,3)*pow(1-p2c3_acc,1)
					: (p1' = p1 - p2c3_dmg) & (p1c1' = p1c1 - p2c3_dmg) & (p1c3' = p1c3 - p2c3_dmg) & (action' = 39) + 
		pow(p2c3_acc,3)*pow(1-p2c3_acc,1)
					: (p1' = p1 - p2c3_dmg) & (p1c1' = p1c1 - p2c3_dmg) & (p1c2' = p1c2 - p2c3_dmg) & 
					(action' = 39) + 
		pow(p2c3_acc,2)*pow(1-p2c3_acc,2)
					: (p1' = p1 - p2c3_dmg) & (p1c1' = p1c1 - p2c3_dmg) + 
		pow(p2c3_acc,2)*pow(1-p2c3_acc,2)
					: (p1' = p1 - p2c3_dmg) & (p1c2' = p1c2 - p2c3_dmg) + 
		pow(p2c3_acc,2)*pow(1-p2c3_acc,2)
					: (p1' = p1 - p2c3_dmg) & (p1c3' = p1c3 - p2c3_dmg) +
		pow(p2c3_acc,2)*pow(1-p2c3_acc,2)
