		pow(p2c3_acc,1)*pow(1-p2c3_acc,1)
					: (p1' = p1 - p2c3_dmg) & (action' = 39) + 
		pow(1-p2c3_acc,2)	: (action' = 39);

	[p2c3_attacks_all]	action = 35 & p1c1 <= 0 & p1c2 <= 0 & p1c3 <= 0 & p1 > 0 ->
		p2c3_acc		: (p1' = p1 - p2c3_dmg) & (action' = 39) +
		1-p2c3_acc		: (action' = 39);

	[next_turn]		action = 39 & turn_clock > 0 & p1 > 0 & p2 > 0 ->
					(turn_clock' = 3 - turn_clock) & (action' = 0);
endmodule

// Single initial state
label "p1_win" = (p1 > 0 & p2 <= 0);
label "p2_win" = (p2 > 0 & p1 <= 0);
